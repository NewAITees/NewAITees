{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "curly": ["error", "all"],
    "comma-dangle": ["error", "always-multiline"],
    "no-unused-vars": "warn"
  }
} 